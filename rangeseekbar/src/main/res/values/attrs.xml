<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSlider">
        <attr name="rs_trackColor" format="color" />
        <attr name="rs_trackSelectedColor" format="color" />
        <attr name="rs_trackThickness" format="dimension" />
